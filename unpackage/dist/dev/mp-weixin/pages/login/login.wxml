<view class="content"><image class="logo" src="/static/logo.png"></image><view><view class="input-fa"><input class="log-input" placeholder="用户名/邮箱/手机号" data-event-opts="{{[['input',[['__set_model',['$0','phone','$event',[]],['form']]]]]}}" value="{{form.phone}}" bindinput="__e"/></view><view class="input-fa"><input class="log-input" placeholder="请输入密码" password="{{true}}" data-event-opts="{{[['input',[['__set_model',['$0','password','$event',[]],['form']]]]]}}" value="{{form.password}}" bindinput="__e"/></view></view><view class="flex justify-center" style="margin-top:40rpx;"><button data-event-opts="{{[['tap',[['login',['$event']]]]]}}" class="uni-bg-red round" style="width:650rpx;" bindtap="__e">登录</button></view><view class="flex justify-between input-fa" style="margin-top:30rpx;"><text>新用户注册</text></view></view>